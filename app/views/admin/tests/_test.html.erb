<tr>
  <td><a href="<%= admin_test_path(test) %>"><%= test.title %></a></td>
  <td><%= test.level %></td>
  <td><%=  test.author.first_name unless test.author.nil? %></td>
  <td>
    <%= link_to t('edit'), edit_admin_test_path(test), :class => "btn btn-primary" %>
    <%= link_to t('delete'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: 'Are you sure?' },
                class: "btn btn-danger" %>
  </td>
</tr>
