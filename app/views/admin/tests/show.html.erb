<div class="container py-5">
  <div class="row">
    <div class="offset-md-3 col-md-6">
      <p><strong><%= t('.title') %>: </strong><%= @test.title %></p>
       <p><strong><%= t('.level') %>: </strong><%= @test.level %></p>
      <h4 class="mt-4"><%= t('.header') %></h4>
      <table class="table">
        <%= render partial: 'admin/questions/question', collection: @test.questions %>
      </table>
      <%= link_to t('create'), new_admin_test_question_path(@test), :class => "btn btn-success" %>
      <div class="d-flex justify-content-between my-3">
        <%= link_to t('back'), admin_tests_path, :class => "btn btn-secondary" %>
        <%= link_to t('edit'), edit_admin_test_path(@test), :class => "btn btn-primary" %>
      </div>
    </div>
  </div>
</div>
